{
    "name": "affine",
    "version": "1.1.0",
    "description": "A plugin for Quaver, designed to automate and simplify timing lines.",
    "main": "config.js",
    "bin": "config.js",
    "pkg": {
        "assets": [
            "views/**/*"
        ],
        "output": "./",
        "options": [
            "experimental-modules"
        ]
    },
    "keywords": [
        "SV",
        "Quaver",
        "Quaver Timing Lines",
        "Quaver Timing Points"
    ],
    "author": "ESV Sweetplum",
    "license": "ISC",
    "scripts": {
        "build": "pkg . --output config --public-packages \"*\""
    },
    "dependencies": {
        "chalk": "^4.1.2"
    }
}
